<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Board">
    <select id="getList" resultType="com.kedu.board.dto.BoardDTO">
        select id,writer,title,write_date from boards;
    </select>

    <select id="getBoardById" parameterType="int" resultType="com.kedu.board.dto.BoardDTO">
        select * from boards where id=#{id}
    </select>
</mapper>